(load "math.so"
  ((c_cos cos)
  (c_sin sin)
  (c_tan tan)
  (c_sqrt sqrt)
  (c_ceil ceil)
  (c_floor floor)))

(define (abs x)
  ((lambda (zero)
    (if (< x zero)
      (- zero x)
      x))
    (if (integer? x)
      0
      0.0)))

(define (pow x n)
  (if (= n 0)
    1
    (if (= n 1)
      x
      (* x (pow x (- n 1))))))
